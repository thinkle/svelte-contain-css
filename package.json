{
	"name": "contain-css-svelte",
	"version": "0.0.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"deploy": "vite build && npx gh-pages -d build",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"css": "./dist/vars/defaults.css"
		},
		"./vars/*": "./dist/vars/*",
		"./themes/*": "./dist/vars/themes/*",
		"./controls/": "./dist/controls/",
		"./layout/": "./dist/layout/",
		"./dropdowns/": "./dist/dropdowns/",
		"./typography/": "./dist/typography/",
		"./misc": "./dist/misc/"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.17",
		"@sveltejs/package": "^2.3.2",
		"gh-pages": "^6.1.1",
		"publint": "^0.2.8",
		"sass": "^1.77.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.1",
		"svelte-preprocess": "^6.0.1",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5",
		"vite": "5.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.2",
		"prismjs": "^1.29.0",
		"svelte-actions-resize": "^0.1.1",
		"svelte-portal": "^2.2.1",
		"svelte-prism": "^1.1.6"
	}
}
